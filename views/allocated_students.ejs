<%- include('partials/prof-header') -%>

<%#  comment  %>

<div class="jumbotron">
    <h1> View Allocated students </h1> 
</div>

<div style="text-align: center; margin-bottom: 30px; margin-top: 50px;">
   
</div>

<div class="box">
    <h2>Hi Prof. <%= username %> </h2> <br>
    <h5> Project list:</h5>
    <% var len = Math.floor(student_list.length) ; %>
    <% for(var i=0; i<len ; i++){ %>
            <% var student = student_list[i] %>
            <p> <%= student.allocated_proj_id %> </p>
            <p> <%= student.student_id %> </p>
            <p> <%= student.email %> </p>
            <br><br>
    <% } %>
</div>




